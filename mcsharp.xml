<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="data/CompilerSettingsSchema.xsd" type="application/xml" schematypens="http://www.w3.org/2001/XMLSchema"?>
<compiler>
    <target>
        <min>1.15</min>
        <max>1.15</max>
        <mismatch-policy>lowest</mismatch-policy>
    </target>
    <defines>
        <define name="hello" value="world"/>
    </defines>
    <build>
        <entry-points>
            <function>init</function>
        </entry-points>
        <loops>
            <function>main</function>
        </loops>
        <output>/out/</output>
        <obfuscation-policy>
            <functions>increment</functions>
            <variables>letters</variables>
        </obfuscation-policy>
    </build>
    <dependencies>
        <dependency>
            <author>Keheck</author>
            <repository>MCSharp</repository>
        </dependency>
    </dependencies>
</compiler>