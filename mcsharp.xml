<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="data/CompilerSettingsSchema.xsd" type="application/xml" schematypens="http://www.w3.org/2001/XMLSchema"?>
<compiler>
    <target>
        <min>1.15</min>
        <max>1.15</max>
        <mismatch-policy>lowest</mismatch-policy>
    </target>
    <defines>
        <define name="hello" value="world"/>
    </defines>
    <build>
        <dir>
            <code>sample/src/</code>
            <lib>sample/lib/</lib>
            <data>sample/data/</data>
        </dir>
    </build>
    <dependencies>
        <dependency>
            <author>Keheck</author>
            <repository>MCSharp</repository>
        </dependency>
    </dependencies>
</compiler>